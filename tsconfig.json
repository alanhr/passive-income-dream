{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "compilerOptions": {
    "importsNotUsedAsValues": "remove",
    "module": "ESNext",
    "sourceMap": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules/*", "__sapper__/*", "public/*"]
}
